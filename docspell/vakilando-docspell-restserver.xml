  <Icon>https://raw.githubusercontent.com/vakilando/unraid-docker-templates/main/docspell/docspell-logo.png</Icon>
  <ExtraParams></ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1608493151</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>Docker images for Docspell. https://github.com/eikek/docspell   Converted By Community Applications   Always verify this template (and values) against the dockerhub support page for the container</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>7880</HostPort>
        <ContainerPort>7880</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/opt/docspell.conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>SomeRandomString</Value>
      <Name>DOCSPELL_HEADER_VALUE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>postgresql</Value>
      <Name>DB_TYPE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>db</Value>
      <Name>DB_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>5432</Value>
      <Name>DB_PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>dbname</Value>
      <Name>DB_NAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>dbuser</Value>
      <Name>DB_USER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>somesafepassword</Value>
      <Name>DB_PASS</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Port 1" Target="7880" Default="7880" Mode="tcp" Description="Container Port: " Type="Port" Display="always" Required="false" Mask="false">7880</Config>
  <Config Name="docspell.conf" Target="/opt/docspell.conf" Default="" Mode="rw" Description="Container Path: /opt/docspell.conf" Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="DOCSPELL_HEADER_VALUE" Target="DOCSPELL_HEADER_VALUE" Default="SomeRandomString" Mode="" Description="DOCSPELL_HEADER_VALUE=" Type="Variable" Display="always" Required="true" Mask="false">SomeRandomString</Config>
  <Config Name="DB_TYPE" Target="DB_TYPE" Default="postgresql" Mode="" Description="DB_TYPE=postgresql&#13;&#10;The database used" Type="Variable" Display="always" Required="true" Mask="false">postgresql</Config>
  <Config Name="DB_HOST" Target="DB_HOST" Default="db" Mode="" Description="DB_HOST=db" Type="Variable" Display="always" Required="true" Mask="false">db</Config>
  <Config Name="DB_PORT" Target="DB_PORT" Default="5432" Mode="" Description="DB_PORT=5432 (postgres)" Type="Variable" Display="always" Required="true" Mask="false">5432</Config>
  <Config Name="DB_NAME" Target="DB_NAME" Default="dbname" Mode="" Description="DB_NAME=dbname" Type="Variable" Display="always" Required="true" Mask="false">dbname</Config>
  <Config Name="DB_USER" Target="DB_USER" Default="dbuser" Mode="" Description="DB_USER=dbuser" Type="Variable" Display="always" Required="true" Mask="false">dbuser</Config>
  <Config Name="DB_PASS" Target="DB_PASS" Default="somesafepassword" Mode="" Description="DB_PASS=somesafepassword" Type="Variable" Display="always" Required="true" Mask="false">somesafepassword</Config>
